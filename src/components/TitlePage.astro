---
import Title from './Title.astro'
import SpowloIcon from './icons/SpowloIcon.astro'
type Props = {
	title: string
}

const { title } = Astro.props
---

<div class='flex justify-start align-middle items-center gap-2 title'>
	<SpowloIcon style='background-color: #1DB954; border-radius: 0.250rem' />
	<Title>{title}</Title>
</div>
